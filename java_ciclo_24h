import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		
		Scanner sc = new Scanner(System.in);
		
		int A, B, tempo;
		
		A = sc.nextInt();
		B = sc.nextInt();
		
		if (A < B) {
			tempo = B - A;
		}
		else {
			tempo = 24 - A + B;
		}
		
		System.out.println("O JOGO DUROU " + tempo + " Horas");
		
		sc.close();
	}
}
